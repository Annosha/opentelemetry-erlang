{erl_opts, [debug_info]}.
{deps, [grpcbox,
        opentelemetry,
        opentelemetry_api]}.

{grpc, [{protos, ["opentelemetry-proto/opentelemetry/proto/collector/trace/v1"]},
        {gpb_opts, [{module_name_prefix, "opentelemetry_exporter_"},
                    {module_name_suffix, "_pb"},
                    {i, "apps/opentelemetry_exporter/opentelemetry-proto/"}]}]}.
